# Medical Desk Advanced - Variáveis de Ambiente

# Porta do servidor
PORT=8080

# Banco de dados (mesmo cluster do TELEMED)
DATABASE_URL=postgres://user:pass@host:5432/telemed

# JWT (chave pública do TELEMED para validação)
JWT_PUBLIC_KEY_PEM="-----BEGIN PUBLIC KEY-----\nMIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8AMIIBCgKCAQEA...\n-----END PUBLIC KEY-----\n"
JWT_AUDIENCE=telemed
JWT_ISSUER=telemed-auth

# CORS origens permitidas
CORS_ORIGINS=http://localhost:3000,https://telemed-deploy-ready.onrender.com

# Feature flag (controle no TELEMED)
MDA_ENABLED=false

# S3 para arquivos/PDFs
S3_ACCESS_KEY_ID=your-access-key
S3_SECRET_ACCESS_KEY=your-secret-key
S3_BUCKET=telemed-mda-files
S3_REGION=us-east-1

# OpenAI para IA (opcional)
OPENAI_API_KEY=your-openai-key