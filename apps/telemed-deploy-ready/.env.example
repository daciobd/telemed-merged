# TeleMed - MedicalDesk Integration
# Copy this to .env and configure with your values

# Enable/disable MedicalDesk feature
FEATURE_MEDICALDESK=true

# MedicalDesk service base URL (without trailing slash)
MEDICALDESK_URL=https://seu-medicaldesk.exemplo.com

# JWT secret for session tokens
# IMPORTANTE: Use uma chave forte e aleatória em produção (mínimo 32 caracteres)
# Gere com: openssl rand -base64 32
JWT_SECRET=YOUR_STRONG_RANDOM_SECRET_HERE

# Server port (default: 5000)
PORT=5000
