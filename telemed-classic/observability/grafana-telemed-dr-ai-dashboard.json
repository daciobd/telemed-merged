  "panels": [
    {
      "type": "timeseries",
      "title": "AI Latency (p50/p90/p99) por modelo",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le, model) (rate(ai_latency_ms_bucket{model=~\"$model\"}[5m])))",
          "legendFormat": "p50 {{model}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.9, sum by (le, model) (rate(ai_latency_ms_bucket{model=~\"$model\"}[5m])))",
          "legendFormat": "p90 {{model}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, model) (rate(ai_latency_ms_bucket{model=~\"$model\"}[5m])))",
          "legendFormat": "p99 {{model}}",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Chamadas de IA por minuto (por modelo/fallback)",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (model, fallback) (rate(ai_attempts_total{model=~\"$model\"}[5m]))",
          "legendFormat": "{{model}} (fallback={{fallback}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Fallback utilizado por minuto",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(ai_fallback_used_total[5m])",
          "legendFormat": "fallback usado/min",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Top 5 Rate Limit Blocks",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "topk(5, sum by (key) (rate(rate_limit_blocks_total{key=~\"$key\"}[5m])))",
